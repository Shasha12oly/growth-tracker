# Environment Configuration for Growth Tracker Pro

# Node Environment
NODE_ENV=production

# Server Configuration
PORT=10000

# CORS Configuration (Update with your Render URL)
ALLOWED_ORIGIN=https://your-app-name.onrender.com

# Security Headers
ENABLE_SECURITY_HEADERS=true

# Cache Configuration
ENABLE_CACHING=true
CACHE_DURATION=31536000

# Logging
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=false
